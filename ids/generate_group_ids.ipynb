    "import json\n",
    "import shortuuid\n",
    "\n",
    "NUM_PARTICIPANTS = 200\n",
    "\n",
    "ids = {str(shortuuid.ShortUUID().random(length=12)): (i % 2 == 0) for i in range(NUM_PARTICIPANTS)}\n",
    "\n",
    "with open(\"ids2.json\", \"w\") as fp:\n",
    "    json.dump(ids, fp, indent=4)"
   ]
